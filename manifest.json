{
  "manifest_version": 2,
  "name": "Searginator",
  "description": "A Translator That is One Click Away",
  "version": "1.0",
  "homepage_url" : "https://github.com/Sht128/Searginator",
  "icons": {
    "48": "icons/icon.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "Translation Popup",
    "default_popup": "popup/popup.html"
  }
}
